{
  "name": "vehicle",
  "displayName": "Vehicle",
  "fields": [
    {
      "name": "vehicle_type",
      "title": "Vehicle Type",
      "type": "select",
      "width": 3,
      "values": [
        "Car",
        "SUV",
        "Pickup",
        "Motorcycle"
      ]
    },
    {
      "name": "body_style",
      "title": "Body Style",
      "type": "select",
      "width": 3,
      "values": [
        "2 door",
        "4 door"
      ],
      "condition": {
        "vehicle_type": [
          "Car"
        ]
      }
    },
    {
      "name": "motorcycle_cc",
      "title": "CC",
      "width": 3,
      "type": "number",
      "condition": {
        "vehicle_type": [
          "Motorcycle"
        ]
      }
    },
    {
      "name": "vin",
      "title": "Vehicle Identification Number (eg  2HGEJ6526VH515413  )",
      "type": "string",
      "newline": true,
      "optional": true
    },
    {
      "newline": true,
      "name": "year",
      "title": "Vehicle Year",
      "type": "string",
      "regex": "^(19[3-9][0-9]|2[0-9]{3})$",
      "width": 3
    },
    {
      "name": "make",
      "title": "Vehicle Make",
      "type": "string",
      "width": 3
    },
    {
      "name": "model",
      "title": "Vehicle Model",
      "type": "string",
      "width": 3
    },
    {
      "newline": true,
      "name": "vehicle_value",
      "title": "Vehicle Value",
      "type": "number",
      "width": 3,
      "decimalPlaces": 0
    },
    {
      "name": "unrepaired_damage",
      "title": "Unrepaired Damage",
      "width": 3,
      "optional": true,
      "type": "select",
      "values": [
        "No - Existing Damage",
        "Yes - Existing Damage"
      ]
    },
    {
      "name": "ownership_indicator",
      "title": "Ownership Indicator",
      "width": 3,
      "optional": true,
      "type": "select",
      "values": [
        "Owned - Not Financed",
        "Owned - Financed",
        "Leased"
      ]
    },
    {
      "name": "vehicle_plated_state",
      "title": "Licensed State",
      "newline": true,
      "type": "select",
      "width": 3,
      "optional": true,
      "values": [
        "AL",
        "AK",
        "AZ",
        "AR",
        "CA",
        "CO",
        "CT",
        "DE",
        "FL",
        "GA",
        "HI",
        "ID",
        "IL",
        "IN",
        "IA",
        "KS",
        "KY",
        "LA",
        "ME",
        "MD",
        "MA",
        "MI",
        "MN",
        "MS",
        "MO",
        "MT",
        "NE",
        "NV",
        "NH",
        "NJ",
        "NY",
        "NC",
        "ND",
        "OH",
        "OK",
        "OR",
        "PA",
        "RI",
        "SC",
        "SD",
        "TN",
        "TX",
        "UT",
        "VT",
        "VA",
        "WA",
        "WV",
        "WI",
        "WY"
      ]
    },
    {
      "name": "license_plate",
      "title": "License Plate Number",
      "type": "string",
      "width": 3,
      "optional": true
    },
    {
      "newline": true,
      "name": "annual_miles",
      "title": "Annual Miles",
      "width": 3,
      "type": "number",
      "optional": true
    },
    {
      "name": "primary_vehicle_use",
      "title": "Primary Vehicle Use",
      "width": 3,
      "optional": true,
      "type": "select",
      "values": [
        "Pleasure Only",
        "Drive to Work / School",
        "Business - Sales - Artisan",
        "Farm"
      ]
    },
    {
      "name": "travel_radius",
      "title": "Travel Radius",
      "width": 3,
      "type": "select",
      "values": [
        "Less than 10 Miles",
        "Between 11 to 20 Miles",
        "Over 20 Miles"
      ],
      "condition": {
        "primary_vehicle_use": [
          "Business - Sales - Artisan"
        ]
      }
    },
    {
      "name": "travel_radius_farm",
      "title": "Radius",
      "width": 3,
      "type": "select",
      "values": [
        "Within 150 miles of Farm",
        "Intra State",
        "Multiple States"
      ],
      "condition": {
        "primary_vehicle_use": [
          "Farm"
        ]
      }
    },
    {
      "name": "distance_to_work_school",
      "title": "Distance One Way",
      "width": 3,
      "type": "select",
      "values": [
        "Less than 10 Miles",
        "Between 11 to 20 Miles",
        "Over 20 Miles"
      ],
      "condition": {
        "primary_vehicle_use": [
          "Drive to Work / School"
        ]
      }
    },
    {
      "name": "vehicle_photos",
      "title": "Vehicle VIN and Pre-Insurance Documentation ",
      "type": "media",
      "width": 12,
      "repeatable": true,
      "optional": true
    }
  ],
  "externalDataCalls": [
    {
      "triggerField": "vin",
      "targetFields": [
        "make",
        "model",
        "year"
      ],
      "uri": "https://qg9x69rbi9.execute-api.us-east-2.amazonaws.com/prod/"
    }
  ]
}